{
  "name": "lw-react-components",
  "private": true,
  "version": "v1.0.0",
  "description": "React components used by me in my projects",
  "author": "Nikita Moshensky",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./dist ./coverage",
    "build": "npm run clean && npm run tsc && npm run build:prod",
    "build:prod": "NODE_ENV=production webpack --mode production --config ./config/webpack.config.js",
    "build:ab": "ANALYZE_BUNDLE=true npm run build",
    "build:dll": "npm run clean && webpack --config ./config/webpack.dll.config.js",
    "start": "npm run build:dll && npm run start:dev",
    "start:dev": "nodemon --config ./config/nodemon.json",
    "tsc": "tsc --noEmit --pretty",
    "tsc:watch": "npm run tsc -- --watch",
    "test": "LOG_LEVEL=error jest --config ./config/jest/jest.json -w4",
    "test:watch": "LOG_LEVEL=error jest --watch --config ./config/jest/jest.json",
    "test:visual": "loki test laptop -- --port 9001",
    "test:approve-visual": "loki approve",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write .eslintrc.js .prettierrc.json tsconfig.json './config/**/*.{js,ts,json}' ",
    "intl:extract": "gulp --gulpfile ./config/gulpfile.js intl:extract",
    "intl:manage": "gulp --gulpfile ./config/gulpfile.js intl:manage",
    "intl": "npm run intl:extract && npm run intl:manage",
    "storybook": "start-storybook -p 9001 -c ./config/storybook --ci",
    "storybook:build": "build-storybook -c ./config/storybook -o storybook-static --quiet",
    "update-deps": "npx npm-check-updates --configFileName .ncurc.js",
    "knip": "knip"
  },
